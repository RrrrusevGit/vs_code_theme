{
	"$schema": "vscode://schemas/color-theme",
	"name": "Silvi Midnight",
	"include": "./silvi-midnight-base.json",
	"tokenColors": [
	  {
		"name": "PHP variables",
		"scope": "variable.other.php",
		"settings": { "foreground": "#dfc3a1" }
	  }
	],
	"semanticTokenColors": {
		"class": "#4EC9B0",
		"class.declaration": "#4EC9B0",
  
		"function": "#dfc3a1",
		"function.declaration": "#dfc3a1",
		"method": "#dfc3a1",
		"method.declaration": "#dfc3a1",
  
		"variable.local": "#A8C8B2",
		"variable.parameter": "#A8C8B2",
		"variable.property": "#A8C8B2",
		"variable.member": "#A8C8B2",
		"variable": "#A8C8B2",
		"variable.declaration": "#A8C8B2",
		"variable.readonly": "#A8C8B2",
		"variable.readonly.local": "#A8C8B2",
		"variable.readonly.declaration": "#A8C8B2",
		
		"property": "#C4BEB6",
		"property.readonly": "#C4BEB6"
	},
	"colors": {
		"editor.background": "#2e2f30",
		"editor.foreground": "#dfc3a1",
		"editor.lineHighlightBackground": "#373737",
		"editor.lineHighlightBorder": "#373737",
		"editor.selectionBackground": "#1d545c",
		"editor.selectionForeground": "#bec0c2",
		"editorCursor.foreground": "#dfc3a1",
		"editorWhitespace.foreground": "#bec0c2",
		"editorIndentGuide.background": "#404244",
		"editorIndentGuide.activeBackground": "#bec0c2",
		"editorIndentGuide.background1": "#404244",
		"editorIndentGuide.activeBackground1": "#bec0c2",
		"editor.findMatchBackground": "#8a7f2c",
		"editor.findMatchHighlightBackground": "#8a7f2c80",
		"editor.findRangeHighlightBackground": "#36365580",
		"editorBracketMatch.background": "#1d545c",
		"editorBracketMatch.border": "#bec0c2",
		"editor.wordHighlightBackground": "#8a402c80",
		"editor.wordHighlightStrongBackground": "#8a402c80",
		"editorError.foreground": "#d65645",
		"editorWarning.foreground": "#d6c545",
		"editorLineNumber.foreground": "#bec0c2",
		"editorLineNumber.activeForeground": "#d6c540",
		"sideBar.background": "#353537",
		"activityBar.background": "#2d2d30",
		"statusBar.background": "#007acc",
		"panel.background": "#2d2d2e",
		"input.background": "#404244",
		"input.foreground": "#efeae5",
		"inputOption.activeBorder": "#45c6d6",
		"dropdown.background": "#404244",
		"dropdown.foreground": "#dfc3a1",
		"list.activeSelectionBackground": "#1d545c",
		"list.activeSelectionForeground": "#bec0c2",
		"list.hoverBackground": "#373737",
		"list.inactiveSelectionBackground": "#1d545c",
		"scrollbarSlider.background": "#404244",
		"scrollbarSlider.hoverBackground": "#404244",
		"scrollbarSlider.activeBackground": "#404244",
		"editorBracketHighlight.foreground1": "#C4BEB6",
		"editorBracketHighlight.foreground2": "#C4BEB6",
		"editorBracketHighlight.foreground3": "#C4BEB6",
		"editorBracketHighlight.foreground4": "#C4BEB6",
		"editorBracketHighlight.foreground5": "#C4BEB6",
		"editorBracketHighlight.foreground6": "#C4BEB6"
	}
}
